---
layout: none
date: 2013-12-12 00:00:00 +0530
---
<?xml version="1.0" encoding="UTF-8"?>
<rss xmlns:media = "http://search.yahoo.com/mrss/" version="2.0">
	<channel>
		<title>{{ site.title }}</title>
		<link>{{ site.url }}</link>
		<description>{{ site.description }}</description>
		<category>Blogs</category>
		<copyright>Uncopyrighted. Free to the world.</copyright>
		<image>
			<url>{{site.url}}/img/Premkumar_Masilamani.jpg</url>
			<title>{{ site.author }}</title>
			<link>{{ site.url }}</link>
		</image>
		<language>en-us</language>
		<managingEditor>{{ site.email }}</managingEditor>
		<webMaster>{{ site.email }}</webMaster>
		<author>{{ site.email }}</author>
		{% for post in site.posts %}
			<item>
				<title>{{ post.title | xml_escape }}</title>
				<link>{{ site.url }}{{ post.url }}</link>
				<description>{{ post.content | xml_escape }}</description>
			{% if post.author != null %}
				<author>{{ post.author }}</author>
			{% else %}
				<author>{{ site.author }}</author>
			{% endif %}
				<category>{{ post.category }}</category>
				<!-- added for testing media RSS -->
				<media:content type="image/jpeg"> 	  
					<media:title>Drew Barrymore</media:title>
					<media:description>Gorgeous Drew Barrymore</media:description>
					<media:thumbnail url="{{site.url}}/img/drew-barrymore.jpg" height="1200" width="1920"> </media:thumbnail>	  
				</media:content>
				<pubDate>{{ post.date | date: "%a, %d %b %Y %H:%M:%S %z" }}</pubDate>
				<guid isPermaLink="true">{{ site.url }}{{ post.url }}</guid>
			</item>
		{% endfor %}
	</channel>
</rss>
